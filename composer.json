{
    "name": "invaders-xx/filament-kanban-widget",
    "description": "Add kanban widget to your pages",
    "keywords": [
        "invaders-xx",
        "laravel",
        "filament",
        "kanban",
        "widget",
        "filament-kanban-widget"
    ],
    "homepage": "https://github.com/invaders-xx/filament-kanban-widget",
    "license": "MIT",
    "authors": [
        {
            "name": "David Vincent",
            "email": "envahisseur@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "filament/filament": "^2.0",
        "spatie/laravel-package-tools": "^1.13.5",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "InvadersXX\\FilamentKanbanWidget\\": "src",
            "InvadersXX\\FilamentKanbanWidget\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InvadersXX\\FilamentKanbanWidget\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "InvadersXX\\FilamentKanbanWidget\\FilamentKanbanWidgetServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
